pipeline{
    agent any 
    
    environment {
        PATH = "C:\\Softwares\\apache-maven-3.6.3\\bin:$PATH"
    }
    
    stages {
        stage("Git Checkout") {
            steps {
                git credentialsId: 'bf820a9c-ccf2-4045-b9c1-7c41df40ddcd', url: 'https://github.com/92dinesh-yadav/spring-jenkins'
            }
        }
        
        stage("Maven Build") {
            steps{
                sh "mvn clean package"
            }
        }
        
    }
}
